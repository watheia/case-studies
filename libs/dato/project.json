{
  "root": "libs/dato",
  "sourceRoot": "libs/dato/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/node:package",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/dato",
        "tsConfig": "libs/dato/tsconfig.lib.json",
        "packageJson": "libs/dato/package.json",
        "main": "libs/dato/src/index.ts",
        "assets": ["libs/dato/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/dato/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/dato"],
      "options": {
        "jestConfig": "libs/dato/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
